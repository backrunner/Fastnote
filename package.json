{
  "dependencies": {
    "ajv": "^6.10.2",
    "buffer-from": "^1.1.1",
    "electron-titlebar": "0.0.3",
    "electron-updater": "^4.2.0",
    "locate-path": "^5.0.0",
    "source-map": "^0.7.3",
    "webfontloader": "^1.6.28",
    "electron-json-storage": "^4.1.8"
  },
  "devDependencies": {
    "animate.css": "^3.7.2",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "bootstrap": "^4.3.1",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "font-awesome": "^4.7.0",
    "gulp-qiniu-up": "^1.0.2",
    "html5sortable": "^0.9.17",
    "jquery": "^3.4.1",
    "merge": "^1.2.1",
    "moment": "^2.24.0",
    "popper.js": "^1.16.0"
  },
  "name": "fastnote",
  "version": "0.2.15",
  "homepage": "https://note.pwp.app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --x64",
    "build32": "electron-builder --ia32",
    "test": "electron ."
  },
  "author": "BackRunner",
  "repository": {
    "type": "git",
    "url": "https://github.com/backrunner/Fastnote.git"
  },
  "license": "GPLv3",
  "build": {
    "files": [
      "!.vscode/**/*",
      "!**/Readme*",
      "!**/gulpfile*.js",
      "!start.bat",
      "!old_version/**/*",
      "!icon.ico"
    ],
    
    "bugs":{
      "url": "",
      "email": "dev@backrunner.top"
    },
    "appId": "app.pwp.fastnote",
    "compression": "maximum",
    "productName": "Fastnote",
    "publish": [
      {
        "provider": "generic",
        "url": "http://update.backrunner.top/fastnote"
      }
    ],
    "win": {
      "target": {
        "target": "nsis"
      },
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "differentialPackage": false,
      "createDesktopShortcut": true
    }
  },
  "description": ""
}
