    "electron-builder": "^22.5.1",
    "font-awesome": "^4.7.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-less": "^4.0.1",
    "gulp-qiniu-up": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
