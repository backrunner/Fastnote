{
  "name": "fastnote",
  "version": "0.4.0",
  "homepage": "https://note.pwp.app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "gulp build",
    "watch": "gulp watch",
    "publish": "gulp publish",
    "publish:hotfix": "gulp publish-hotfix",
    "build:win64": "electron-builder --x64",
    "build:win32": "electron-builder --ia32",
    "test": "electron ."
  },
  "author": "BackRunner",
  "dependencies": {
    "ajv": "^6.12.6",
    "buffer-from": "^1.1.1",
    "electron-json-storage": "^4.2.0",
    "electron-updater": "^4.3.5",
    "locate-path": "^5.0.0",
    "pako": "^1.0.11",
    "request": "^2.88.2",
    "source-map": "^0.7.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "animate.css": "^4.1.1",
    "asar": "^3.0.3",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "bootstrap": "^4.5.3",
    "del": "^6.0.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "font-awesome": "^4.7.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-git": "^2.10.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-less": "^4.0.1",
    "gulp-qiniu-up": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.8.0",
    "gulp-uglify": "^3.0.2",
    "html5sortable": "^0.9.18",
    "jquery": "^3.5.1",
    "lozad": "^1.16.0",
    "marked": "^1.2.2",
    "merge": "^1.2.1",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pwp-app/Fastnote.git"
  },
  "bugs": {
    "url": "https://github.com/pwp-app/Fastnote/issues",
    "email": "dev@backrunner.top"
  },
  "license": "GPL-3.0-or-later",
  "build": {
    "files": [
      "!.vscode/**/*",
      "!**/Readme*",
      "!**/gulpfile*.js",
      "!start.bat",
      "!old_version/**/*",
      "!icon.ico",
      "!assets/**/*",
      "!bug.md",
      "!qiniu.config.js",
      "!sign.config.js",
      "!hotfix.log.json",
      "!gulpfile.js",
      "!.gitignore",
      "!hotfix/**/*",
      "!modules/**/*"
    ],
    "appId": "app.pwp.fastnote",
    "compression": "maximum",
    "productName": "Fastnote",
    "forceCodeSigning": true,
    "publish": [
      {
        "provider": "generic",
        "url": "http://update.backrunner.top/fastnote"
      }
    ],
    "win": {
      "target": {
        "target": "nsis"
      },
      "icon": "icon.ico",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "publisherName": "pwp.app",
      "rfc3161TimeStampServer": "http://timestamp.digicert.com"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "differentialPackage": false,
      "createDesktopShortcut": true
    }
  },
  "description": ""
}
