  "name": "fastnote",
  "version": "0.3.1",
  "homepage": "https://note.pwp.app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --x64",
    "build32": "electron-builder --ia32",
    "test": "electron ."
  },
  "author": "BackRunner",
  "repository": {
    "type": "git",
    "url": "https://github.com/pwp-app/Fastnote.git"
  },
  "bugs": {
    "url": "https://github.com/pwp-app/Fastnote/issues",
    "email": "dev@backrunner.top"
  },
  "license": "GPLv3",
  "build": {
    "files": [
      "!.vscode/**/*",
      "!**/Readme*",
      "!**/gulpfile*.js",
      "!start.bat",
      "!old_version/**/*",
      "!icon.ico",
      "!assets/**/*",
      "!bug.md",
      "!qiniu.config.js",
      "!sign.config.js",
      "!hotfix.log.json",
      "!gulpfile.js",
      "!.gitignore",
      "!hotfix/**/*"
    ],
    "appId": "app.pwp.fastnote",
    "compression": "maximum",
    "productName": "Fastnote",
    "publish": [
      {
        "provider": "generic",
        "url": "http://update.backrunner.top/fastnote"
      }
    ],
    "win": {
      "target": {
        "target": "nsis"
      },
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "differentialPackage": false,
      "createDesktopShortcut": true
    }
  },
  "description": ""
}
